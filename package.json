{
  "name": "@nabla/vite-plugin-eslint",
  "description": "Plugs ESLint into Vite dev server",
  "version": "2.0.6",
  "license": "MIT",
  "author": "Arnaud Barr√© (https://github.com/ArnaudBarre)",
  "type": "module",
  "main": "src/index.cjs",
  "types": "src/index.d.ts",
  "module": "src/index.mjs",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "require": "./src/index.cjs",
      "import": "./src/index.mjs"
    }
  },
  "files": [
    "src"
  ],
  "repository": "github:nabla/vite-plugin-eslint",
  "keywords": [
    "vite",
    "eslint",
    "vite-plugin"
  ],
  "scripts": {
    "prettier": "prettier --write '**/*.{js,ts,json,md,yml}'"
  },
  "dependencies": {
    "@types/eslint": "*",
    "chalk": "^4",
    "debug": "^4"
  },
  "peerDependencies": {
    "eslint": "^8.45 || ^9",
    "vite": "^4 || ^5 || ^6 || ^7"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "prettier": "3.0.3",
    "vite": "^5.2.9"
  }
}
